# Common shortcuts
alias reload="source ~/.zshrc"
# Directory traversal
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias ~="cd ~"
alias -- -="cd -"
alias wp="cd ~/workspace"




